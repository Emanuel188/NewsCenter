@model NewsCenter.Areas.Administrador.Models.EliminarNoticiaViewModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "EliminarNoticia";
}

<!DOCTYPE html>
<html>
<head>
    <title>Eliminar Noticia - NewsCenter</title>
    <link rel="stylesheet" href="~/styles.css">
</head>
<body>

    <main>
        <h1>Eliminar Noticia</h1>
        <p class="noticia-titulo">@Model.Titulo</p>
       <form action="~/administrador/home/eliminarNoticia" method="post">
            <input type="hidden" asp-for="Id" />
            <div class="acciones">
                <button type="submit" class="delete">Eliminar</button>
                <a href="~/administrador/home/index" class="cancel">Cancelar</a>
            </div>
        </form>
    </main>

</body>
</html>


